            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') SOCKS
        div.collapse.navbar-collapse(id='bs-example-navbar-collapse-1')
          ul.nav.navbar-nav.navbar-right
            if (!user)
              li
                a(href='/login') Login
              li
                a(href='/register') Register
            if (user)
              li
                a Welcome 
                  b #{user.username}
              li
                a(href='/logout') Logout

    div.container
      div.content
        block content
      

